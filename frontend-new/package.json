	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev-host": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
